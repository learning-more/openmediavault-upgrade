#!/bin/sh

PLUGINS="
cups
dnsmasq
docker-gui
domoticz
duplicati
eyefi
ldap
letsencrypt
mumble
mysql
netatalk
nginx
openvpn
plexmediaserver
pxe
remotedesktop
route
shellinabox
syncthing
transmissionbt
urbackup-server
vdo
vdr
vdr-extras
vdr-vnsiserver
virtualbox
webdav
"

set -e

for p in ${PLUGINS}
do
	dpkg -l | awk '/^ii/ { print $2 }' | grep openmediavault-${p} && apt-get autoremove --purge --yes openmediavault-${p}
done

exit 0
